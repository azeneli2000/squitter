<section>
	<div id="stories" class="storiesWrapper"></div>
</section>
	<section class="section1 line">
		<div class="container">
			<div class="row">
				<h1 class="t-upper c-red">Models</h1>

				<!-- start: girls -->
				<div class="col-md-12" id="galery0">
					<div class="row">
						<% allGirls.forEach(function(entry) {%>
						<div class="col-sm-4 col-md-4 col-lg-3 wow zoomIn" data-wow-delay="0">
							<div class="gallery-icon">
								<img src="uploads/<%=entry.id%>-profile.jpg" alt="img">
								<div class="gallery-caption">
									<strong><%=entry.name%></strong>
									<p><%=entry.years%> years</p>
									<p><%=entry.hobby%></p>
									<a href="#" id= <%=entry.id%> onclick="reply_click(this.id); return false;"  class="main-button">View Profile</a>
								</div>
							</div>
						</div>
						<%});%>						
					</div>
				</div>
			
				<!-- end: girls -->
	
				<!-- start: pagination -->
				<div class="col-md-12 pagination">
					<ul>
						<li><a href="#">&laquo;</a></li>
						<li id="l1"><a onclick="show1(1);hide(0);showActive(1);hideActive(2); return false;"  href="#">1</a></li>
						<li id = "l2"  onclick="show1(0);hide(1);showActive(2);hideActive(1); return false;"  class="active"><a href="#">2</a></li>
						<li><a href="#">3</a></li>
						<li><a href="#">4</a></li>
						<li><a href="#">5</a></li>
						<li><a href="#">&raquo;</a></li>
					</ul>
				</div>
				<!-- end: pagination -->


			</div>
		</div>
	</section>

	<section class="section2 line">
		<div class="container">
			<div class="row">
				<h1 class="t-upper">Blog</h1>

				<!-- start: article-container -->
				<div class="col-md-8 col-md-offset-2 col-lg-12 col-lg-offset-0 articles-container">
					<div class="row">
                        <% allGirls.allBlogs.forEach(function(entry) {%>
						<div class="col-sm-4 col-md-4 col-lg-5ths article wow zoomIn" data-wow-delay="0">					
							<a href="http://185.158.2.13/blog/<%=entry.id%>">								
								<img src="blogs/<%=entry.id%>-preview.jpg" alt="">
								<h3><%=entry.name%></h3>
								<p style="overflow: hidden;display: -webkit-box;-webkit-line-clamp: 7;-webkit-box-orient: vertical;">
								<%=entry.about%></p>
							</a>
						</div>
                        <%});%> 
					</div>
				</div>
				<!-- end: article-container -->				
			</div>
		</div>
	</section>

