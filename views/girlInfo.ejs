		//storie

		document.getElementById('storieUpload').addEventListener('change', handleFileSelectStory, false);
		function handleFileSelectStory(evt) {
			var files = evt.target.files; // FileList object

			var currentSkin = getCurrentSkin();
			var stories = new Zuck('stories', {
				backNative: false,
				openEffect: true,
				previousTap: true,
				skin: currentSkin['name'],
				autoFullScreen: true,
				avatars: currentSkin['params']['avatars'],
				paginationArrows: currentSkin['params']['paginationArrows'],
				list: currentSkin['params']['list'],
				cubeEffect: currentSkin['params']['cubeEffect'],
				localStorage: true,
				stories: []
			});
			for (var i = 0, f; f = files[i]; i++) {
				var reader = new FileReader();
				reader.onload = (function (theFile) {
					return function (e) {
						if (e.target.result.match("video")) {
							console.log("video");
							let item = Zuck.buildTimelineItem(
								i,
								"/uploads/<%=json.id%>/profile/<%=json.id%>-profile-detail.jpg",
								"andi",
								"https://ramon.codes",
								timestamp(),
								[
									[i, "video", 0, e.target.result, "", false, timestamp()],

								]
							)
							stories.update(item);
						}
						if (e.target.result.match("image")) {
							let item = Zuck.buildTimelineItem(
								i,
								"/uploads/<%=json.id%>/profile/<%=json.id%>-profile-detail.jpg",
								"andi",
								"https://ramon.codes",
								timestamp(),
								[
									[i, "photo", , e.target.result, "", false, timestamp()],

								]
							)
							stories.update(item);
						}
					};
				})(f);
				// Read in the image file as a data URL.
				reader.readAsDataURL(f);
			}
		}
